rt { serialize } from \"@/helpers.js\";\n\nexport default {\n  data() {\n    return {\n      produtos: null,\n      produtosPorPaginas: 9\n    };\n  },\n  computed: {\n    url() {\n      const query = serialize(this.$router.query);\n      return `/produto?_limit=${this.produtosPorPaginas}${query}`;\n    },\n  },\n   methods: {\n    getProdutos() {\n      api.get(this.url).then(response => {\n        this.produtos = response.data;\n      });\n    }\n  },\n  watch: {\n    url() {\n      this.getProdutos();\n    }\n  },\n  created() {\n    this.getProdutos();\n  },\n};\n</script>\n\n<style scoped>\n</style>\n\n"]}]}